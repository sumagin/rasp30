#! /usr/bin/vvp
:ivl_version "0.9.5 " "(v0_9_5)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa050130 .scope module, "test_div2" "test_div2" 2 30;
 .timescale 0 0;
v0xa04cb40_0 .var "clk", 0 0;
v0xa04cbc0_0 .net "op1", 0 0, v0xa04ca78_0; 1 drivers
v0xa04cc30_0 .var "resett", 0 0;
S_0xa02ca80 .scope module, "div_clk" "div2" 2 48, 2 1, S_0xa050130;
 .timescale 0 0;
v0xa02cb00_0 .var "ctr", 2 0;
v0xa04ca18_0 .net "ip1", 0 0, v0xa04cb40_0; 1 drivers
v0xa04ca78_0 .var "op1", 0 0;
v0xa04cad8_0 .net "reset", 0 0, v0xa04cc30_0; 1 drivers
E_0xa050290 .event posedge, v0xa04ca18_0;
    .scope S_0xa02ca80;
T_0 ;
    %wait E_0xa050290;
    %load/v 8, v0xa04cad8_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xa04ca78_0, 0, 0;
    %ix/load 0, 3, 0;
    %assign/v0 v0xa02cb00_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0xa02cb00_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v0xa02cb00_0, 0, 8;
    %load/v 8, v0xa02cb00_0, 3;
    %cmpi/u 8, 7, 3;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v0xa04ca78_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0xa04ca78_0, 0, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v0xa02cb00_0, 0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xa050130;
T_1 ;
    %set/v v0xa04cb40_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0xa050130;
T_2 ;
    %delay 1, 0;
    %set/v v0xa04cc30_0, 0, 1;
    %delay 20, 0;
    %set/v v0xa04cc30_0, 1, 1;
    %delay 20, 0;
    %set/v v0xa04cc30_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0xa050130;
T_3 ;
    %delay 5, 0;
    %load/v 8, v0xa04cb40_0, 1;
    %inv 8, 1;
    %set/v v0xa04cb40_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0xa050130;
T_4 ;
    %vpi_call 2 55 "$dumpfile", "test.vcd";
    %vpi_call 2 56 "$dumpvars", 1'sb0, S_0xa050130;
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "tb_div2.v";
